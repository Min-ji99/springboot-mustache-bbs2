{{>layouts/header}}
<div>
    이 페이지는 리스트 페이지 입니다.<br>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            <th scope="col">Content</th>
        </tr>
        </thead>
        <tbody>
        {{#articles}}
            <tr>
                <th scope="row">{{id}}</th>
                <td><a href="/articles/{{id}}">{{title}}</a></td>
                <td>{{content}}</td>
            </tr>
        {{/articles}}
        </tbody>
    </table>
</div>
{{>layouts/footer}}